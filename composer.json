{
  "name": "tephida/corner",
  "description": "Generic PHP Exceptions and Errors, made user-friendly.",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Tephida\\Corner\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tephida\\Corner\\Tests\\": "test/"
    }
  },
  "require": {
    "php": "^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "psalm/plugin-phpunit": "<1",
    "vimeo/psalm": "^5"
  }
}
